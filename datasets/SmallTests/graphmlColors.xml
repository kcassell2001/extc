<?xml version="1.0" encoding="UTF-8"?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns"  
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns 
        http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd">

  <!-- Node attributes -->
  <key id="label" for="node" attr.type="string"/>
  <key id="memberType" for="node" attr.type="string">
    <default>method</default>
  </key>

  <!-- Edge attributes -->
  <key id="score" for="edge" attr.type="double"/>
<!--
  <key id="d1" for="edge" attr.name="weight" attr.type="double"/>
-->
  <graph id="G" edgedefault="undirected">
    <node id="n0">
      <data key="label">green</data>
    </node>
    <node id="n1"/>
    <node id="n2">
      <data key="label">blue</data>
    </node>
    <node id="n3">
      <data key="label">red</data>
    </node>
    <node id="n4"/>
    <node id="n5">
      <data key="label">turquoise</data>
    </node>

    <edge id="e0" source="n0" target="n2">
      <data key="score">1.0</data>
    </edge>
    <edge id="e1" source="n0" target="n1">
      <data key="score">1.0</data>
    </edge>
    <edge id="e2" source="n1" target="n3">
      <data key="score">2.0</data>
    </edge>
    <edge id="e3" source="n3" target="n2"/>
    <edge id="e4" source="n2" target="n4"/>
    <edge id="e5" source="n3" target="n5"/>
    <edge id="e6" source="n5" target="n4">
      <data key="score">1.1</data>
    </edge>
  </graph>
</graphml>
